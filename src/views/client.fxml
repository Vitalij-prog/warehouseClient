<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<TabPane fx:controller="controllers.ClientController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="Profile">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="labelUserName" layoutX="145.0" layoutY="44.0" text="Label">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Label>
                        <Label layoutX="33.0" layoutY="44.0" text="user name:">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Label>
                    </children></AnchorPane>
            </content>
        </Tab>
        <Tab text="Products">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TableView fx:id="prodTableView" layoutX="164.0" layoutY="33.0" prefHeight="271.0" prefWidth="406.0">
                            <columns>
                                <TableColumn fx:id="prodCol_1" prefWidth="74.0" text="id" />
                                <TableColumn fx:id="prodCol_2" prefWidth="142.0" text="name" />
                                <TableColumn fx:id="prodCol_3" prefWidth="99.0" text="price" />
                                <TableColumn fx:id="prodCol_4" prefWidth="106.0" text="amount" />
                            </columns>
                        </TableView>
                        <Button fx:id="buttonShowProducts" layoutX="49.0" layoutY="33.0" mnemonicParsing="false" onAction="#showProducts" prefHeight="34.0" prefWidth="70.0" text="Show" />
                        <ChoiceBox fx:id="choiceBoxForSearchingProducts" layoutX="22.0" layoutY="127.0" prefHeight="32.0" prefWidth="123.0" />
                        <Label layoutX="59.0" layoutY="99.0" text="search by" />
                        <Label fx:id="labelProductNotFound" layoutX="44.0" layoutY="216.0" text="data not found" textFill="#e10808" visible="false" />
                        <Button fx:id="buttonSearch" layoutX="51.0" layoutY="240.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="66.0" text="Search" />
                        <TextField fx:id="textForSearching" layoutX="22.0" layoutY="175.0" prefHeight="33.0" prefWidth="123.0" />
                    </children></AnchorPane>
            </content>
        </Tab>
        <Tab text="My orders">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="buttonShow" layoutX="153.0" layoutY="290.0" mnemonicParsing="false" onAction="#selectOrders" text="Show" />
                        <Button fx:id="buttonOrder" layoutX="377.0" layoutY="290.0" mnemonicParsing="false" text="Order" />
                        <TableView fx:id="tableOrders" layoutX="27.0" layoutY="26.0" prefHeight="236.0" prefWidth="546.0">
                            <columns>
                                <TableColumn fx:id="colId" prefWidth="75.0" text="order id" />
                                <TableColumn fx:id="colName" prefWidth="94.0" text="product name" />
                                <TableColumn fx:id="colAmount" prefWidth="61.0" text="amount" />
                                <TableColumn fx:id="colPrice" prefWidth="76.0" text="order price" />
                                <TableColumn fx:id="colDate" prefWidth="77.0" text="date" />
                                <TableColumn fx:id="colTime" prefWidth="77.0" text="time" />
                                <TableColumn fx:id="colStatus" prefWidth="85.0" text="status" />
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
